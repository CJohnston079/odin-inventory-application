<link rel="stylesheet" href="/styles/table.css">
<table>
	<thead>
		<tr>
			<th>Genre</th>
			<th>Books</th>
		</tr>
	</thead>
	<tbody>
		<% genres.map((genre) => { %>
		<tr>
			<td><a href="/genres/<%= genre.id %>/<%= genre.slug %>"><%= genre.name %></a></td>
			<td><%= genre.number_of_books %></td>
		</tr>
		<% }) %>
	</tbody>
</table>
