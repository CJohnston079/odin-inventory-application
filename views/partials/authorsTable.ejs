<table>
	<thead>
		<tr>
			<th>Author</th>
			<th>Born</th>
			<th>Nationality</th>
			<th>Books</th>
			<th>Genres</th>
		</tr>
	</thead>
	<tbody>
		<% authors.map(author => { %>
		<tr>
			<td><a href="authors/<%= author.id %>"><%= author.name %></a></td>
			<td>
				<a href="decades/<%= Math.floor(author.birth_year / 10) %>0s"><%= author.birth_year %></a>
			</td>
			<td><a href="countries/<%= author.country %>"><%= author.nationality %></a></td>
			<td><%= author.number_of_books %></td>
			<%- include("./genresColumn", { data: author }) %>
		</tr>
		<% }) %>
	</tbody>
</table>
